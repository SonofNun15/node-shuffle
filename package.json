{
  "name": "shuffle",
  "version": "1.0.0",
  "description": "Randomly shuffle a list of items",
  "main": "dist/index.js",
  "scripts": {
    "debug": "ts-node ./src/index.ts",
    "build": "npm run clean && tsc",
    "build.prod": "npm run clean && tsc --project tsconfig.prod.json",
    "clean": "rimraf ./dist",
    "lint": "eslint src --ext ts",
    "start": "node ./dist/index.js",
    "test": "tsc && mocha ./dist/**/*.spec.js"
  },
  "keywords": [
    "shuffle",
    "node",
    "typescript"
  ],
  "author": "Josh Graber",
  "license": "MIT",
  "devDependencies": {
    "@types/chai": "^4.2.11",
    "@types/mocha": "^7.0.2",
    "@types/node": "^14.0.13",
    "@types/sinon": "^9.0.4",
    "@typescript-eslint/eslint-plugin": "^3.2.0",
    "@typescript-eslint/parser": "^3.2.0",
    "chai": "^4.2.0",
    "eslint": "^7.2.0",
    "mocha": "^8.0.1",
    "rimraf": "^3.0.2",
    "sinon": "^9.0.2",
    "ts-node": "^8.10.2",
    "typescript": "^3.9.5"
  }
}
